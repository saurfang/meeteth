{
  "name": "meeteth",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.6.4",
    "drizzle-react": "^1.1.1",
    "drizzle-react-components": "^1.1.0",
    "eth-block-tracker-es5": "^2.3.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "moment": "^2.22.2",
    "openzeppelin-solidity": "^1.10.0",
    "prettier": "^1.13.5",
    "react": "^16.4.1",
    "react-big-calendar": "^0.19.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts-antd": "2.15.1",
    "solidity-treemap": "^0.0.1",
    "truffle-contract": "^3.0.6",
    "web3": "^1.0.0-beta.34"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-scripts-ts-antd start",
    "build": "react-scripts-ts-antd build",
    "eject": "react-scripts-ts-antd eject",
    "test": "./scripts/test.sh",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:sol": "solium -d .",
    "lint:sol:fix": "solium -d . --fix",
    "lint:all": "npm run lint && npm run lint:sol",
    "lint:all:fix": "npm run lint:fix && npm run lint:sol:fix",
    "coverage": "./node_modules/.bin/solidity-coverage && cat coverage/lcov.info | node_modules/.bin/coveralls"
  },
  "devDependencies": {
    "@types/jest": "^23.1.0",
    "@types/node": "^10.3.5",
    "@types/react": "^16.4.1",
    "@types/react-big-calendar": "^0.18.0",
    "@types/react-dom": "^16.0.6",
    "@types/react-router-dom": "^4.2.7",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-bignumber": "^2.0.2",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "ganache-cli": "6.1.3",
    "solidity-coverage": "^0.5.4",
    "solium": "^1.1.7",
    "truffle": "^4.1.12",
    "typescript": "^2.9.2"
  },
  "resolutions": {
    "@types/react": "16.4.1"
  }
}